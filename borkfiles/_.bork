destination $HOME/.dotfiles

# Update Bork
ok github frdmn/bork

# All UNIXes
include configs.bork

# OS specfic
case $OSTYPE in
  darwin*)
    include atom.bork
    include brew.bork
    include cask.bork
    include macstore.bork
    include osx.bork
    include pip.bork ;;
esac

# Shell specfic
case $SHELL in
  *zsh*)
    include zsh.bork ;;
esac

# Depending on above
include bash.bork
include gem.bork
include npm.bork
include powerline.bork
include vim.bork
